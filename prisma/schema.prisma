generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Scene{
  id String @id @default(uuid())
  name String?
  imageString String @db.Text
  userId String
  sceneString String @db.Text
  user User @relation("UserToScene",fields:[userId],references: [id])
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

}
model User{
  id String @id @default(uuid())
  name String?
  email String @unique
  scenes Scene[] @relation("UserToScene")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}